{
  "manifest_version": 3,
  "name": "Hightouch Events Debugger",
  "version": "1.0.0",
  "description": "DevTools panel to inspect Hightouch Events SDK requests & payloads (page/track/identify).",
  "minimum_chrome_version": "114",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage"],
  "host_permissions": [
    "https://events.hightouch.com/*"
  ],
  "action": {
    "default_title": "Hightouch Events Debugger"
  }
}